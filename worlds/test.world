<?xml version="1.0" ?>
<sdf version="1.5">
	<world name="default">
	
	<!-- A Global Light source -->
	<include>
		<uri>model://sun</uri>
	</include>
	
	<!-- A ground plane -->
	<include>
		<uri>model://ground_plane</uri>
	</include>
	
	<!--Adding a desktop -->
	<include>
		<static>true</static>
		<pose>0 0 0 0 0 0</pose>
		<uri>model://desktop</uri>
	</include>
	
	</world>
</sdf>
